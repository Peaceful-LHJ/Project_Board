<mapper namespace="com.project.repository.comment.CommentDelRepository">
	
	<insert id="boardCommentDelInsert">
    	insert into comment_DEL (DELcommentNum, commentNum, content, writer, writer, bno, regDate, updateDate)
		select seq_board_DEL.nextval, bno, title, content, writer, regDate, updateDate
		from board where bno = #{bno}
    </insert>
	
</mapper>